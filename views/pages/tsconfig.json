{
  "extends": "astro/tsconfigs/base",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src/",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@constans/*": ["constans/*"],
    }
  },
  "include": [
    "src"
  ] 
}